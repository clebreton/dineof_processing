<graph id="demarine_coarse_grid_projection_nc_writer">
  <version>1.0</version>
  <node id="demarine_coarse_grid_projection">
    <operator>Reproject</operator>
    <sources>
      <source>${source}</source>
    </sources>
    <parameters>
        <wktFile/>
        <crs>GEOGCS["WGS84(DD)", 
          DATUM["WGS84", 
